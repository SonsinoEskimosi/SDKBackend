{
  "name": "sdkbackend",
  "version": "1.0.0",
  "description": "TypeScript SDK backend server",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "build:sdk": "esbuild js_sdk/sdk.ts --bundle --outfile=public/sdk/sdk.js --format=iife",
    "start": "NODE_ENV=production node dist/server.js",
    "predev": "node scripts/archive-logs.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn src/server.ts",
    "watch:sdk": "esbuild js_sdk/sdk.ts --bundle --outfile=public/sdk/sdk.js --format=iife --watch",
    "lint": "eslint \"src/**/*.ts\" --ignore-pattern \"src/sdk/**\"",
    "lint:fix": "eslint \"src/**/*.ts\" --ignore-pattern \"src/sdk/**\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.8",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "openai": "^4.24.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "redis": "^5.10.0",
    "uuid": "^13.0.0",
    "winston": "^3.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@types/redis": "^4.0.10",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
